local function system(text)
game.StarterGui:SetCore("ChatMakeSystemMessage", {
	Text = text;
	Font = Enum.Font.SourceSansLight;
	Color = Color3.new(255, 255, 255);
	FontSize = Enum.FontSize.Size8;
})
end

