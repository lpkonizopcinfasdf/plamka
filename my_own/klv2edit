        wait(.2)
        chat("colorshifttop 260 205 25")
        chat("time 7")
    elseif string.sub(msg:lower(), 0, 7) == prefix.."theme3" then
        chat("fix")
        wait(.2)
        chat("fogend 1000")
        chat("time 0")
        chat("fogcolor 400 0 1000")
    elseif string.sub(msg:lower(), 0, 7) == prefix.."theme4" then
        chat("fix")
        wait(.2)
        chat("outdoorambient 0 0 0")
        chat("time 3")
        chat("brightness 0")
    elseif string.sub(msg:lower(), 0, 7) == prefix.."theme5" then
        chat("fix")
        wait(.2)
        chat("colorshiftbottom 275 75 200")
        chat("colorshifttop 255, 211, 8")
        chat("ambient 250 175 245")
    end
    
    if string.sub(msg:lower(), 0, 6) == prefix.."regen" then
        if game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen") then
            notif("Regen Command", "Pads have been regenerated")
            fireclickdetector(game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").ClickDetector,0)
        else
            notif("Attention!", "Regen pad could not be found.")
        end
    end
    
    if string.sub(msg:lower(), 0, 6) == prefix.."toreg" then
        if game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen") then
            notif("To Regen Command", "Teleported to the Regen.")
            lplr.Character.HumanoidRootPart.CFrame = game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").CFrame * CFrame.new(Vector3.new(0, 4, 0))
        else
            notif("Attention!", "Regen pad could not be found.")
        end
    end
end)

-- Other Things

mouse.KeyDown:Connect(function(key)
    if key:lower() == "z" then
        local target = mouse.Target
        chat("stun me")
        repeat
			if lplr.Character then
				if lplr.Character:FindFirstChild("HumanoidRootPart") then
                    lplr.Character.HumanoidRootPart.CFrame = target.CFrame * CFrame.new(-1*(target.Size.X/2)-(lplr.Character['Torso'].Size.X/2), 0, 0)
                    game:GetService("RunService").Heartbeat:wait()
                    chat("unpunish me")
                end
            end
		until lplr.Character.Torso:FindFirstChild("Weld")
        chat("unstun me")
    end
end)

wait(.5)

if runessentialcommandsonexecute then
    obbydeath = true
    antikilltoggle = true
    antipunishtoggle = true
    antifreezetoggle = true
    antiattachtoggle = true
    antidogtoggle = true
    antispintoggle = true
    antimsgtoggle = true
    antifiretoggle = true
    antitptoggle = false
    antismoketoggle = true
    antiblindtoggle = true
    permtoggle = true
    antijailtoggle = true
    notif("Essential Commands", "Essential Commands have been executed.")
    for i,v in pairs(game.Workspace.Terrain._Game.Workspace.Obby:GetChildren()) do
        v:WaitForChild("TouchInterest"):Destroy()
    end
    spawn(function()
        while antikilltoggle do wait(.05)
            if lplr.Character:WaitForChild("Humanoid").Health <= 0 and game.Workspace:FindFirstChild(lplr.Name) then
                if game.Workspace[lplr.Name]:FindFirstChild("HumanoidRootPart") then
                    chat("reset me")
                    wait(.4)
                end
            end
        end
    end)
    spawn(function()
        while antimsgtoggle do game:GetService("RunService").Heartbeat:wait()
            if lplr.PlayerGui:FindFirstChild("HintGUI") then
                lplr.PlayerGui:FindFirstChild("HintGUI"):Destroy()
            end
            if lplr.PlayerGui:FindFirstChild("MessageGUI") then
                lplr.PlayerGui:FindFirstChild("MessageGUI"):Destroy()
            end
        end
    end)
    spawn(function()
        while antiblindtoggle do wait()
            if lplr.PlayerGui:FindFirstChild("EFFECTGUIBLIND") then
                lplr.PlayerGui.EFFECTGUIBLIND:Destroy()
                wait(.1)
            end
        end
    end)
    spawn(function()
        while antipunishtoggle do wait(.1)
            if game.Lighting:FindFirstChild(lplr.Name) then
                chat("unpunish me")
            end
        end
    end)
    spawn(function()
        while wait() do
            for i,v in ipairs(workspace:GetDescendants()) do
                if v:IsA("BasePart") or v:IsA("Part") then
                    if v.Transparency == 1 and v.Name ~= "HumanoidRootPart" then
                        v.Transparency = 0.6
                    end
                end
            end
        end
    end)
    spawn(function()
        while antispintoggle do wait()
            if lplr.Character then
                if lplr.Character:FindFirstChild("Torso") then
                    if lplr.Character.Torso:FindFirstChild("SPINNER") then
                        lplr.Character.Torso.SPINNER:Destroy()
                        wait(.1)
                    end
                end
            end
        end
    end)
    spawn(function()
        while antitptoggle do wait()
    	    if pos1 == nil then
    			pos1 = lplr.Character.HumanoidRootPart.CFrame
    			pos2 = lplr.Character.HumanoidRootPart.CFrame
    		end
    		
    		local gap = (Vector3.new(pos1.x, pos1.y, pos1.z) - Vector3.new(pos2.x, pos2.y, pos2.z)).Magnitude
    		if math.floor(gap) > 8 then
    			lplr.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(pos1.x, pos1.y, pos1.z))
    			pos1 = lplr.Character.HumanoidRootPart.CFrame
    			pos2 = lplr.Character.HumanoidRootPart.CFrame
    		else
    			pos1 = pos2
    			pos2 = lplr.Character.HumanoidRootPart.CFrame
    		end
    	end
    end)
    spawn(function()
        while antidogtoggle do wait()
            if lplr.Character then
                if lplr.Character:FindFirstChild("Addon") then
                    chat("undog me")
                    wait(.1)
                end
            end
        end
    end)
    spawn(function()
        while antismoketoggle do game:GetService("RunService").Heartbeat:wait()
            if lplr.Character then
                if lplr.Character:FindFirstChild("Torso") then
                    if lplr.Character.Torso:FindFirstChild("Smoke") then
                        lplr.Character.Torso.Smoke:Destroy()
                    end
                end
            end
        end
    end)
    spawn(function()
        while antifiretoggle do game:GetService("RunService").Heartbeat:wait()
            if lplr.Character then
                if lplr.Character:FindFirstChild("Torso") then
                    if lplr.Character.Torso:FindFirstChild("Fire") then
                        lplr.Character.Torso.Fire:Destroy()
                    end
                end
            end
        end
    end)
    spawn(function()
        while antijailtoggle do wait(.05)
            if game.Workspace.Terrain._Game.Folder:FindFirstChild(lplr.Name.."'s jail") then
                chat("unjail me")
            end
        end
    end)
    spawn(function()
        while antifreezetoggle do wait(.05)
            if lplr.Character then
                if lplr.Character:FindFirstChild("ice") then
                    chat("thaw me")
                end
            end
        end
    end)
    spawn(function()
        while permtoggle do wait()
            if not allpadstoggle then
                if not game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild(lplr.Name.."'s admin") then
                    if not game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin") then
                        if game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen") then
                            fireclickdetector(game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").ClickDetector,0)
                        else
                            notif("Attention!", "Regen pad could not be found, turning off perm.")
                            permtoggle = false
                            wait(5)
                        end
                    else
                        local pad = game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin")
                        pad:WaitForChild("Head").Transparency = 1
                        pad.Head.CanCollide = false
                        local previouspadcf = pad.Head.CFrame
                        pad.Head.CFrame = lplr.Character.HumanoidRootPart.CFrame
                        wait()
                        pad.Head.CFrame = previouspadcf
                        pad.Head.CanCollide = true
                        pad.Head.Transparency = 0
                    end
                end
            end
        end
    end)
    spawn(function()
        while antiattachtoggle do wait()
            for i,v in pairs(game.Players:GetPlayers()) do
                if v.Name ~= lplr.Name then
                    if v.Character then
                        for i,ch in pairs(v.Character:GetChildren()) do
                            if not v.Character:FindFirstChild("Addon") then
                                if ch:FindFirstChild("Weld") or ch:FindFirstChild("Snap") then
                                    game:GetService("RunService").Heartbeat:wait()
                                    if string.find(v.Name:lower(), "me_", 1, true) then
                                        chat("reset others")
                                    else
                                        chat("reset "..v.Name)
                                    end
                                    wait(.1)
                              
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
    spawn(function()
        while wait() do
            for p,b in pairs(padbans) do
                for i,v in pairs(game.Workspace.Terrain._Game.Admin.Pads:GetChildren()) do
                    if string.find(v.Name, b, 1, true) then
                        chat("respawn "..b)
                        wait(.1)
                 
                        wait(.1)
                      
                        wait(.5)
                        fireclickdetector(game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").ClickDetector,0)
                    end
                end
            end
        end
    end)
end



wait(.5)

for i, plr in pairs(game.Players:GetPlayers()) do
    if plr.Name ~= lplr.Name then
        if table.find(blacklisted, plr.Name) then
            if string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
                env[plr.Name.."bozobanner"] = workspace.ChildAdded:Connect(function(bozo)
                    if bozo.Name == plr.Name then
                        fireclickdetector(game.Workspace.Terrain["_Game"].Admin:WaitForChild("Regen"):WaitForChild("ClickDetector"),0)
                        chat("punish others")
                        chat("blind others")
                        for i,b in pairs(game.Players:GetPlayers()) do
                            if not table.find(blacklisted, b.Name) then
                                chat("reset "..b.Name)
                                wait(.4)
                            end
                        end
                    end
                end)
                game.Players.ChildRemoved:Connect(function(bozo)
                    if bozo.Name == plr.Name then
                        env[plr.Name.."bozobanner"]:Disconnect()
                    end
                end)
                chat("reset others")
            elseif game.Players:FindFirstChild(plr.Name) then
                env[plr.Name.."bozobanner"] = workspace.ChildAdded:Connect(function(bozo)
                    if bozo.Name == plr.Name then
                        fireclickdetector(game.Workspace.Terrain["_Game"].Admin:WaitForChild("Regen"):WaitForChild("ClickDetector"),0)
                        chat("punish "..plr.Name)
                        chat("blind "..plr.Name)
                    end
                end)
                game.Players.ChildRemoved:Connect(function(bozo)
                    if bozo.Name == plr.Name then
                        env[plr]:Disconnect()
                    end
                end)
                chat("reset "..plr.Name)
            end
        end
        plr.Chatted:Connect(function(msg)
            if string.sub(msg, 0, 9) == "/c system" then
             
            end
            
            if string.sub(msg, 0, 2) == "/w" then
              
            end
        end)
    end
end

game.Players.ChildAdded:Connect(function(plr)
    if table.find(blacklisted, plr.Name) then
        if string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
            env[plr.Name.."bozobanner"] = workspace.ChildAdded:Connect(function(bozo)
                if bozo.Name == plr.Name then
                    fireclickdetector(game.Workspace.Terrain["_Game"].Admin:WaitForChild("Regen"):WaitForChild("ClickDetector"),0)
                    chat("punish others")
                    chat("blind others")
                    for i,b in pairs(game.Players:GetPlayers()) do
                        if not table.find(blacklisted, b.Name) then
                            chat("reset "..b.Name)
                            wait(.4)
                        end
                    end
                end
            end)
            chat("reset others")
        elseif game.Players:FindFirstChild(plr.Name) then
            env[plr.Name.."bozobanner"] = workspace.ChildAdded:Connect(function(bozo)
                if bozo.Name == plr.Name then
                    fireclickdetector(game.Workspace.Terrain["_Game"].Admin:WaitForChild("Regen"):WaitForChild("ClickDetector"),0)
                    chat("punish "..plr.Name)
                    chat("blind "..plr.Name)
                end
            end)
            chat("reset "..plr.Name)
        end
    end
    plr.Chatted:Connect(function(msg)
        if string.sub(msg, 0, 9) == "/c system" then
    
        end
        if string.sub(msg, 0, 2) == "/w" then
 
        end
    end)
    
    if table.find(blacklisted, plr.Name) then
    
    else
        wait(.1)
        chat("name me "..plr.Name.." has joined the server")
    end
end)

if game.ReplicatedStorage:FindFirstChild("WeaponsSystem") then
    game:GetService("Players"):chat("gear me 4842218829")
    wait(1)
    local WeaponsSystem = require(game.ReplicatedStorage.WeaponsSystem.WeaponsSystem)
    local tool = lplr.Backpack.Sniper
    tool.Equipped:Connect(function()
        WeaponsSystem.camera:setEnabled(true)
    end)
    tool.Unequipped:Connect(function()
        WeaponsSystem.camera:setEnabled(false)
    end)
    game:GetService("RunService").Heartbeat:wait(.1)
    lplr.Character.Humanoid:EquipTool(lplr.Backpack.Sniper)
    game:GetService("RunService").Heartbeat:wait(.1)
    lplr.Character.Humanoid:UnequipTools()
    wait(.5)
    game:GetService("Players"):chat("reset me")
end

mouse.KeyDown:Connect(function(key)
    if key:lower() == "r" then
        chat("reset me")
    elseif key:lower() == "f" then
        chat("ff me")
    elseif key:lower() == "t" then
        chat("fix")
    elseif key:lower() == "g" then
        chat("god me")
    elseif key:lower() == "b" then
        chat(prefix.."banhammer")
    elseif key:lower() == "c" and lplr.Backpack:FindFirstChild("Railgun") then
        for i,v in pairs(lplr.Backpack:GetChildren()) do
            v.Parent = lplr.Character
            game:GetService("Players").LocalPlayer.Character.Railgun.Click:FireServer(mouse.Hit.p)
            wait(.025)
            v.Parent = lplr.Backpack
        end
    end
end)

uss.WindowFocused:Connect(function()
    afk = false
end)
uss.WindowFocusReleased:Connect(function()
    afk = true
end)

spawn(function()
    while wait() do
        if afk then
            if not lplr.Character:FindFirstChildOfClass("Model") then
                chat("name me (Tabbed Out)\n"..lplr.DisplayName)
                wait(.5)
            end
            if lplr.Character:FindFirstChildOfClass("Model") then
                if lplr.Character:FindFirstChildOfClass("Model").Name ~= "(Tabbed Out)\n"..lplr.DisplayName then
                    chat("name me (Tabbed Out)\n"..lplr.DisplayName)
                    wait(.5)
                end
            end
        else
            if lplr.Character:FindFirstChildOfClass("Model") then
                chat("unname me")
                wait(.5)
            end
        end
    end
end)



if not game.Workspace.Terrain._Game.Admin:FindFirstChild("Regen") then
    for i, v in pairs(regenpositions) do
        if not game.Workspace.Terrain._Game.Admin:FindFirstChild("Regen") then
            wait(.25)
            lplr.Character.HumanoidRootPart.CFrame = CFrame.new(regenpositions[i])
        else
            break
        end
    end
end





