        chat("gear me 00000000000000000000000000000000000000000000000000000000082357101")
    	chat("gear me 00000000000000000000000000000000000000000000000000000000059190534")
    elseif string.sub(msg:lower(), 0, 8) == prefix.."bhsword" then
        chat("gear me 000000000000000000000000000000000000000000000000000000000268586231")
    elseif string.sub(msg:lower(), 0, 10) == prefix.."rlasergun" then
        chat("gear me 000000000000000000000000000000000000000000000000000000000212296936")
    elseif string.sub(msg:lower(), 0, 10) == prefix.."blasergun" then
        chat("gear me 000000000000000000000000000000000000000000000000000000000130113146")
    elseif string.sub(msg:lower(), 0, 8) == prefix.."machete" then
        chat("gear me 000000000000000000000000000000000000000000000000000000000123234673")
    elseif string.sub(msg:lower(), 0, 11) == prefix.."removemesh" then
        lplr.Character:FindFirstChildOfClass("Tool"):FindFirstChild("Mesh", true):Destroy()
    elseif string.sub(msg:lower(), 0, 10) == prefix.."banhammer" then
        local bancooldown = false
        chat("gear me 00000000000000000000000000000000000000000000000000000000010468797")
        game.Players.LocalPlayer.Backpack:WaitForChild("BanHammer V1.1").Parent = game.Players.LocalPlayer.Character
        wait(.2)
        game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Handle.Touched:Connect(function(hit)
            if hit.Parent:FindFirstChild("Humanoid") or hit.Parent.Parent:FindFirstChild("Humanoid") then
                if not bancooldown then
                    bancooldown = true
                    if hit.Name == "Handle" then
                        if hit.Parent.Parent.Name ~= lplr.Name then
                            game.Players:Chat("invis "..hit.Parent.Parent.Name)
                            wait(.2)
                        
                            wait(.2)
                            game.Players:Chat("clone "..hit.Parent.Parent.Name)
                            game.Players:Chat(prefix.."tban "..hit.Parent.Parent.Name)
                            game.Players:Chat("music 5696182212")
                            wait(1.7)
                            game.Players:Chat("stopmusic")
         
                        end
                    else
                        if hit.Parent.Name ~= lplr.Name then
                            game.Players:Chat("invis "..hit.Parent.Name)
                            wait(.2)
                            game.Players:Chat("name "..hit.Parent.Name.." This user has been banned!")
                            wait(.2)
                            game.Players:Chat("clone "..hit.Parent.Name)
                            game.Players:Chat(prefix.."tban "..hit.Parent.Name)
                            game.Players:Chat("music 5696182212")
                            wait(1.7)
                            game.Players:Chat("stopmusic")
                            game.Players:Chat("removeclones")
                        end
                    end
                    bancooldown = false
                end
            end
        end)
    end
    
    if string.sub(msg:lower(), 0, 3) == prefix.."up" then
        lplr.Character.HumanoidRootPart.CFrame = lplr.Character.HumanoidRootPart.CFrame + Vector3.new(0, 2000, 0)
    elseif string.sub(msg:lower(), 0, 5) == prefix.."down" then
        lplr.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(lplr.Character.HumanoidRootPart.Position.X,7,lplr.Character.HumanoidRootPart.Position.Z))
    end
    
    if string.sub(msg:lower(), 0, 7) == prefix.."theme1" then
        chat("fix")
        wait(.2)
