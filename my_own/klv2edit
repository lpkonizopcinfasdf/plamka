-- Functions

function notif(title ,msg)
    game.StarterGui:SetCore("SendNotification", {
    	Title = title; 
    	Text = msg; 
    	Duration = 5;
    })
end

function chat(text)
    game.Players:Chat(text)
end

ogtext = hookmetamethod(Instance.new("RemoteEvent"), "__namecall", function(Self, ...)
    local args = {...}
    
    if not checkcaller() and getnamecallmethod() == "FireServer" and tostring(Self) == "SayMessageRequest" then
        if string.sub(args[1], 1, #prefix) == prefix then
            game.StarterGui:SetCore("ChatMakeSystemMessage", { Text = "Command has been hidden from other players." })
            return nil
        end
    end
    return ogtext(Self, ...)
end)

workspace.ChildRemoved:Connect(function(loplr)
    if loplr.Name == lplr.Name then
        workspace:WaitForChild(lplr.Name)
        workspace[lplr.Name].DescendantAdded:Connect(function()
            if lplr.Character then
                if lplr.Character:FindFirstChild("Torso") then
                    if lplr.Character.Torso:FindFirstChild("BodyForce") then
                        lplr.Character.Torso:FindFirstChild("BodyForce").Force = Vector3.new(0,0,0)
                        wait()
                        lplr.Character.Torso:FindFirstChild("BodyForce"):Destroy()
                        repeat wait()
                        until not lplr.Character.Torso:FindFirstChild("BodyForce")
                    end
                end
            end
        end)
        if lplr.Character then
            if lplr.Character:FindFirstChild("Humanoid") then
                antispeed = lplr.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
                    lplr.Character.Humanoid.WalkSpeed = 16
                end)
            end
        end
    end
end)

for i,plr in pairs(game.Players:GetPlayers()) do
     if plr.Name ~= lplr.Name then
        plr.CharacterAdded:Connect(function()
            plr.Character.ChildAdded:Connect(function(v)
                if gearblacklist then
                    if not antitoolstoggle then
                    	if table.find(blacklistedgears, v.Name) then
                    		if not string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
                        		chat("removetools                                                                                     "..plr.Name)
                        	
                    		else
                    			chat("removetools                                                                                          others")
                        	
                    		end
                    	end
                    end
                end
            end)
            plr.Backpack.ChildAdded:Connect(function(v)
                if gearblacklist then
                    if not antitoolstoggle then
            			if table.find(blacklistedgears, v.Name) then
            				if not string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
                				chat("removetools                                                                                     "..plr.Name)
                	
            				else
            					chat("removetools                                                                                               others")
                		
            				end
            			end
            		end
            	end
            end)
        end)
    end
end

game.Players.ChildAdded:Connect(function(plr)
    if plr.Name ~= lplr.Name then
        plr.CharacterAdded:Connect(function()
            plr.Character.ChildAdded:Connect(function(v)
                if gearblacklist then
                    if not antitoolstoggle then
                    	if table.find(blacklistedgears, v.Name) then
                    		if not string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
                        		chat("removetools "..plr.Name)
                      
                    		else
                    			chat("removetools others")
                        
                    		end
                    	end
                    end
                end
            end)
            plr.Backpack.ChildAdded:Connect(function(v)
                if gearblacklist then
                    if not antitoolstoggle then
            			if table.find(blacklistedgears, v.Name) then
            				if not string.find(plr.Name:lower(), "me_", 1, true) or string.find(plr.Name:lower(), "all_", 1, true) then
                				chat("removetools "..plr.Name)
                		
            				else
            					chat("removetools others")
                		
            				end
            			end
            		end
            	end
            end)
        end)
    end
end)



-- Commands

lplr.Chatted:Connect(function(msg)
    if string.sub(msg:lower(), 0, 5) == prefix.."spam" then
        spamtoggle = true
        notif("Spam Command", "Spamming "..'"'..string.sub(msg, 7)..'"')
        while spamtoggle do
            game:GetService("RunService").Heartbeat:wait()
            chat(string.sub(msg, 7))
        end
    elseif string.sub(msg:lower(), 0, 5) == prefix.."stop" then
        notif("Spam Command", "Stopped spamming")
        spamtoggle = false
    end
    
    if string.sub(msg:lower(), 0, 6) == prefix.."super" then
        for i = 1, 100 do
            chat(string.sub(msg, 8))
        end
    end
    
    if string.sub(msg:lower(), 0, 7) == prefix.."repeat" then
        if string.split(msg, ",")[4] then
            for i = 1, string.split(msg, ",")[3] do
                chat(string.split(msg, ",")[2])
                wait(string.split(msg, ",")[4])
            end
        else
            for i = 1, string.split(msg, ",")[3] do
                chat(string.split(msg, ",")[2])
            end
        end
    end
    
    if string.sub(msg:lower(), 0, 5) == prefix.."perm" then
        permtoggle = true
        notif("Perm Command", "Toggled on")
        while permtoggle do wait()
            if not allpadstoggle then
                if not game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild(lplr.Name.."'s admin") then
                    if not game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin") then
                        if game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen") then
                            fireclickdetector(game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").ClickDetector,0)
                        else
                            notif("Attention!", "Regen pad could not be found, turning off perm.")
                            permtoggle = false
                            wait(5)
                        end
                    else
                        local pad = game.Workspace.Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin")
                        pad:WaitForChild("Head").Transparency = 1
                        pad.Head.CanCollide = false
                        local previouspadcf = pad.Head.CFrame
                        pad.Head.CFrame = lplr.Character.HumanoidRootPart.CFrame
                        wait()
                        pad.Head.CFrame = previouspadcf
                        pad.Head.CanCollide = true
                        pad.Head.Transparency = 0
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 7) == prefix.."noperm" then
        permtoggle = false
        notif("Perm Command", "Toggled off")
    end
    
    if string.sub(msg:lower(), 0, 8) == prefix.."allpads" then
        allpadstoggle = true
        notif("All Pads Command", "Toggled on")
        while allpadstoggle do wait()
            for i,v in pairs(game.Workspace.Terrain["_Game"].Admin.Pads:GetChildren()) do
                spawn(function()
                    if v.Name == "Touch to get admin" then
                        v.Head.Transparency = 1
                        v.Head.CanCollide = false
                        local previouspadcf = v.Head.CFrame
                        v.Head.CFrame = lplr.Character.HumanoidRootPart.CFrame
                        wait(0)
                        v.Head.CFrame = previouspadcf
                        v.Head.CanCollide = true
                        v.Head.Transparency = 0
                    end
                end)
                if v.Name ~= lplr.Name.."'s admin" then
                    if game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen") then
                        fireclickdetector(game.Workspace.Terrain["_Game"].Admin:FindFirstChild("Regen").ClickDetector,0)
                    else
                        notif("Attention!", "Regen pad could not be found, turning off allpads.")
                        allpadstoggle = false
                        wait(5)
                        break
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 10) == prefix.."noallpads" then
        allpadstoggle = false
        notif("All Pads Command", "Toggled off")
    end
    
    if string.sub(msg:lower(), 0, 11) == prefix.."antiattach" then
        antiattachtoggle = true
        notif("Anti Attach Command", "Toggled off")
        while antiattachtoggle do wait()
            for i,v in pairs(game.Players:GetPlayers()) do
                if v.Name ~= lplr.Name then
                    if v.Character then
                        for i,ch in pairs(v.Character:GetChildren()) do
                            if not v.Character:FindFirstChild("Addon") then
                                if ch:FindFirstChild("Weld") or ch:FindFirstChild("Snap") then
                                    game:GetService("RunService").Heartbeat:wait()
                                    if string.find(v.Name, "me_", 1, true) then
                                        chat("reset others")
                                    else
                                        chat("reset "..v.Name)
                                    end
                                    wait(.1)
                              
                                end
                            end
                        end
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 13) == prefix.."noantiattach" then
        antiattachtoggle = false
        notif("Anti Attach Command", "Toggled off")
    end
    
    if string.sub(msg:lower(), 0, 7) == prefix.."padban" then
        for i,v in pairs(game.Players:GetPlayers()) do
            if string.find(v.Name:lower(), string.sub(msg:lower(), 9), 1, true) then
                if string.find(string.sub(v.Name:lower(), 0, #string.split(msg, " ")[2]), string.split(msg, " ")[2], 1, true) then
                    notif("Pad Ban Command", "Pad Banned "..v.Name)
                    table.insert(padbans, v.Name)
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 9) == prefix.."unpadban" then
        for i,v in pairs(game.Players:GetPlayers()) do
            if string.find(v.Name:lower(), string.sub(msg:lower(), 11), 1, true) then
                if string.find(string.sub(v.Name:lower(), 0, #string.split(msg, " ")[2]), string.split(msg, " ")[2], 1, true) then
                    for p,b in pairs(padbans) do
                        if b == v.Name then
                            notif("Pad Ban Command", "Unpad Banned "..b)
                            table.remove(padbans, p)
                        end
                    end
                end
            end
        end
    end
    
    if string.sub(msg:lower(), 0, 11) == prefix.."antipunish" then
        if antipunishtoggle then
            notif("Anti Punish Command", "Anti Punish has already been toggled")
        else
            antipunishtoggle = true
            notif("Anti Punish Command", "Toggled on")
            while antipunishtoggle do wait()
                if game.Lighting:FindFirstChild(lplr.Name) then
                    chat("unpunish me")
                    wait(.1)
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 13) == prefix.."noantipunish" then
        if not antipunishtoggle then
            notif("Anti Punish Command", "Anti Punish has not been toggled yet")
        else
            antipunishtoggle = false
            notif("Anti Punish Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 8) == prefix.."antimsg" then
        if antimsgtoggle then
            notif("Anti Message Command", "Anti Message has already been toggled")
        else
            antimsgtoggle = true
            notif("Anti Message Command", "Toggled on")
            while antimsgtoggle do game:GetService("RunService").Heartbeat:wait()
                if lplr.PlayerGui:FindFirstChild("HintGUI") then
                    lplr.PlayerGui:FindFirstChild("HintGUI"):Destroy()
                end
                if lplr.PlayerGui:FindFirstChild("MessageGUI") then
                    lplr.PlayerGui:FindFirstChild("MessageGUI"):Destroy()
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 13) == prefix.."noantimsg" then
        if not antimsgtoggle then
            notif("Anti Message Command", "Anti Message has not been toggled yet")
        else
            antimsgtoggle = false
            notif("Anti Message Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 10) == prefix.."antigears" then
        if antitoolstoggle then
            notif("Anti Tools Command", "Anti Tools has already been toggled")
        else
            antitoolstoggle = true
            notif("Anti Tools Command", "Toggled on")
            while antitoolstoggle do wait()
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v.Name ~= lplr.Name then
                        if v.Character then
                            if v.Character:FindFirstChildOfClass("Tool") then
                                chat("removetools others "..v.Name)
                            
                            end
                        elseif v.Backpack then
                            if v.Backpack:FindFirstChildOfClass("Tool") then
                                chat("removetools others "..v.Name)
                              
                            end
                        end
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 12) == prefix.."unantigears" then
        if not antitoolstoggle then
            notif("Anti Tools Command", "Anti Tools has not been toggled yet")
        else
            antitoolstoggle = false
            notif("Anti Tools Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 9) == prefix.."antikill" then
        if antikilltoggle then
            notif("Anti Kill Command", "Anti Kill has already been toggled")
        else
            antikilltoggle = true
            notif("Anti Kill Command", "Toggled on")
            while antikilltoggle do wait()
                if lplr.Character.Humanoid.Health <= 0 and game.Workspace:FindFirstChild(lplr.Name) then
                    if game.Workspace[lplr.Name]:FindFirstChild("HumanoidRootPart") then
                        chat("reset me")
                        wait(.4)
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 11) == prefix.."noantikill" then
        if not antikilltoggle then
            notif("Anti Kill Command", "Anti Kill has not been toggled yet")
        else
            antikilltoggle = false
            notif("Anti Kill Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 9) == prefix.."antispin" then
        if antispintoggle then
            notif("Anti Spin Command", "Anti Spin has already been toggled")
        else
            antispintoggle = true
            notif("Anti Spin Command", "Toggled on")
            while antispintoggle do wait()
                if lplr.Character then
                    if lplr.Character:FindFirstChild("Torso") then
                        if lplr.Character.Torso:FindFirstChild("SPINNER") then
                            lplr.Character.Torso.SPINNER:Destroy()
                            wait(.1)
                        end
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 11) == prefix.."noantispin" then
        if not antispintoggle then
            notif("Anti Spin Command", "Anti Spin has not been toggled yet")
        else
            antispintoggle = false
            notif("Anti Spin Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 9) == prefix.."antijail" then
        if antijailtoggle then
            notif("Anti Jail Command", "Anti Jail has already been toggled")
        else
            antijailtoggle = true
            notif("Anti Jail Command", "Toggled on")
            while antijailtoggle do wait()
                if game.Workspace.Terrain._Game.Folder:FindFirstChild(lplr.Name.."'s jail") then
                    chat("unjail me")
                    wait(.1)
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 11) == prefix.."noantijail" then
        if not antijailtoggle then
            notif("Anti Jail Command", "Anti Jail has not been toggled yet")
        else
            antijailtoggle = false
            notif("Anti Jail Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 11) == prefix.."antifreeze" then
        if antifreezetoggle then
            notif("Anti Freeze Command", "Anti Freeze has already been toggled")
        else
            antifreezetoggle = true
            notif("Anti Freeze Command", "Toggled on")
            while antifreezetoggle do wait()
                if lplr.Character then
                    if lplr.Character:FindFirstChild("ice") then
                        chat("reset                                                                        me")
                        wait(.1)
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 13) == prefix.."noantifreeze" then
        if not antifreezetoggle then
            notif("Anti Freeze Command", "Anti Freeze has not been toggled yet")
        else
            antifreezetoggle = false
            notif("Anti Freeze Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 8) == prefix.."antidog" then
        if antidogtoggle then
            notif("Anti Dog Command", "Anti Dog has already been toggled")
        else
            antidogtoggle = true
            notif("Anti Dog Command", "Toggled on")
            while antidogtoggle do wait()
                if lplr.Character then
                    if lplr.Character:FindFirstChild("Addon") then
                        chat("undog me")
                        wait(.1)
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 10) == prefix.."noantidog" then
        if not antidogtoggle then
            notif("Anti Dog Command", "Anti Dog has not been toggled yet")
        else
            antidogtoggle = false
            notif("Anti Dog Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 9) == prefix.."antifire" then
        if antifiretoggle then
            notif("Anti Fire Command", "Anti Fire has already been toggled")
        else
            antifiretoggle = true
            notif("Anti Fire Command", "Toggled on")
            while antifiretoggle do game:GetService("RunService").Heartbeat:wait()
                if lplr.Character then
                    if lplr.Character.Torso:FindFirstChild("Fire") then
                        lplr.Character.Torso.Fire:Destroy()
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 11) == prefix.."noantifire" then
        if not antifiretoggle then
            notif("Anti Fire Command", "Anti Fire has not been toggled yet")
        else
            antifiretoggle = false
            notif("Anti Fire Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 10) == prefix.."antismoke" then
        if antismoketoggle then
            notif("Anti Smoke Command", "Anti Smoke has already been toggled")
        else
            antismoketoggle = true
            notif("Anti Smoke Command", "Toggled on")
            while antismoketoggle do game:GetService("RunService").Heartbeat:wait()
                if lplr.Character then
                    if lplr.Character.Torso:FindFirstChild("Smoke") then
                        lplr.Character.Torso.Smoke:Destroy()
                    end
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 12) == prefix.."noantismoke" then
        if not antismoketoggle then
            notif("Anti Smoke Command", "Anti Smoke has not been toggled yet")
        else
            antismoketoggle = false
            notif("Anti Smoke Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 10) == prefix.."antiblind" then
        if antiblindtoggle then
            notif("Anti Blind Command", "Anti Blind has already been toggled")
        else
            antiblindtoggle = true
            notif("Anti Blind Command", "Toggled on")
            while antiblindtoggle do wait()
                if lplr.PlayerGui:FindFirstChild("EFFECTGUIBLIND") then
                    lplr.PlayerGui.EFFECTGUIBLIND:Destroy()
                    wait(.1)
                end
            end
        end
    elseif string.sub(msg:lower(), 0, 12) == prefix.."noantiblind" then
        if not antiblindtoggle then
            notif("Anti Blind Command", "Anti Blind has not been toggled yet")
        else
            antiblindtoggle = false
            notif("Anti Blind Command", "Toggled off")
        end
    end
    
    if string.sub(msg:lower(), 0, 7) == prefix.."antitp" then
        if not antitptoggle then
            antitptoggle = true
            notif("Anti Teleport Command", "Anti Teleport has been toggled on.")
            while antitptoggle do wait()
        	    if pos1 == nil then
        			pos1 = lplr.Character.HumanoidRootPart.CFrame
        			pos2 = lplr.Character.HumanoidRootPart.CFrame
        		end
        		
        		local gap = (Vector3.new(pos1.x, pos1.y, pos1.z) - Vector3.new(pos2.x, pos2.y, pos2.z)).Magnitude
        		if math.floor(gap) > 8 then
        			lplr.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(pos1.x, pos1.y, pos1.z))
        			pos1 = lplr.Character.HumanoidRootPart.CFrame
        			pos2 = lplr.Character.HumanoidRootPart.CFrame
        		else
        			pos1 = pos2
        			pos2 = lplr.Character.HumanoidRootPart.CFrame
        		end
            end
    	else
    	    notif("Anti Teleport Command", "Anti Teleport has already been toggled on.")
    	end
